{#
/**
* @file
* Default theme implementation to display the file entity as a video tag.
*
* Available variables:
* - attributes: An array of HTML attributes, intended to be added to the
*   video tag.
* - files: And array of files to be added as sources for the video tag. Each
*   element is an array with the following elements:
*   - file: The full file object.
*   - source_attributes: An array of HTML attributes for to be added to the
*     source tag.
*
* @ingroup themeable
*/
#}
<div id="key_wrap">
<div class="hero-banner--video_wrapper_all" id="keyVisual">
	<div class="hero-banner--video_wrapper videoBox">
		{{ attach_library('classy/file') }}
		<video style="pointer-events: none;" class="hero-banner--video" id="hero-banner--video" controls="false" autoplay loop muted>
		{# code for auto play on mobile : use below #}
		{# <video style="pointer-events: none;" class="hero-banner--video" id="hero-banner--video" controls="false" autoplay="autoplay" loop="loop" muted defaultMuted playsinline> #}
			{% for file in files %}
				<source {{ file.source_attributes }}/>
			{% endfor %}
		</video>
	</div>
	<div class="pause-button_wrapper">
		<div class="container">
			<div id="pause-button" class="pause-button">
				<span class="icon-pausetrendy"></span>
			</div>
		</div>
	</div>
</div>
</div>

<div class="container edit-page-tabs">
	{{ drupal_block('local_tasks_block') }}
</div>

