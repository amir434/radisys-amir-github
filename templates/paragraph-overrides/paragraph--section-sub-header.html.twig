{# paragraph: section sub header #}

{%
  set classes = [
    'section-sub header',
    'paragraph',
    'paragraph--type--' ~ paragraph.bundle|clean_class,
    view_mode ? 'paragraph--view-mode--' ~ view_mode|clean_class,
    not paragraph.isPublished() ? 'paragraph--unpublished'
  ]
%}
    {% block content %}
             <h3 {{ attributes.addClass(classes,'sub-header') }}>{{ content.field_section_sub_header}}</h3>
   {% endblock %}
